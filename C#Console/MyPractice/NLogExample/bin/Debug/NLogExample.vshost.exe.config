<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <nlog>
    <targets>
      <target name="con" type="Console"></target>
    </targets>
    <rules>
      <logger name="*" levels="Info,Debug,Warn" appendTo="con"></logger>
    </rules>
  </nlog>
</configuration>